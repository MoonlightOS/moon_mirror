pkgname=sunny-calamares-config
pkgver=1
pkgrel=1
pkgdesc='SunnyOS Calamares config.'
arch=('any')
license=('GPL-3.0-or-later')
source=(git+https://github.com/openai-ae/sunnyos-calamares-config.git)
